#权重表
wight_config = config/weight.csv

#查询时需要用到的城市代码
areaid_list = config/citylist.csv

#心知天气URL链接
url_header = https://api.thinkpage.cn/v2/weather/all.json?city=
url_tail = &unit=c&aqi=city&key=

#查询城市天气的语言类型
lang_CN = zh-chs
lang_EN = en

#获取天气预报城市的数量
city_num = 50

#thinkpage key 免费注册的key需要及时更换
key=QN2R25G3IW

#城市中文名称
citylist = config/city_list.txt

#代理服务器地址、端口，不使用代理的话在HttpCaller.java去掉相应设置
proxyFlag = true
proxyHost = XX.XX.XX.XX
proxyPort = 8080

#Redis地址、端口
redisHost = 10.6.85.67
redisPort = 6379

#设置抓取数据的间隔sleep时间，暂时没有设置sleep，暂时备用
sleepTime=100

#设置某个城市数据连续抓取次数，超过此数值则跳过此城市
skipNum=10

#天气情况整合文件目录
dumpFileDir = data

#log4j config file
log4jcfg = config/log4j.cfg